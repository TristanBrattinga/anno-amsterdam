{
  "name": "anno-amsterdam",
  "description": "Anno Amsterdam is an application to explore Amsterdam and its beautiful old buildings.",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "storybook": "turbo run storybook",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "config": "*",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "turbo": "^1.13.3"
  },
  "packageManager": "yarn@4.2.2",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "nohoist": [
      "typesafe-i18n",
      "**/typesafe-i18n",
      "**/typesafe-i18n/**"
    ]
  }
}
